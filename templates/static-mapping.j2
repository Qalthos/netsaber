service {
    dhcp-server {
        shared-network-name LAN {
            subnet 192.168.1.0/24 {
                {% for item in query('dict', static_ips) -%}
                static-mapping {{ item.key }} {
                    ip-address {{ item.value.ip_address }}
                    mac-address {{ item.value.mac_address }}
                }
                {% endfor %}
            }
        }
    }
}

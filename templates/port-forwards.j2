port-forward {
    {% for item in query('subelements', vlans.0.dhcp.static_ips, 'forwards', dict(skip_missing=True)) -%}
    rule {{ loop.index - 1 }} {
        description {{ item.1.desc }}
        forward-to {
            address 192.168.1.{{ item.0.ip_address }}
            port {{ item.1.port }}
        }
        original-port {{ item.1.port }}
        protocol tcp
    }
    {% endfor %}
}
